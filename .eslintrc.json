{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "rules": {
    "no-unused-vars": 0,
    "no-console": 0,
    "quotes": ["error", "double"],
    "semi": ["error", "always"]
  }
}
